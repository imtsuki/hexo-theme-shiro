
<article id="post">
<header>
  <h1><%=page.title%></h1>
  <div class="meta">
  <span class="page-date">
  <time datetime="<%=page.date.toJSON()%>">
            <%=date(page.date)%>
  </time>
  </span>
  <% page.tags.each(tag => { %>
  <span class="tag-link">
        <a href="<%=url_for(tag.path)%>">
          #<%=tag.name%>
        </a>
  </span>
      <% }); %>
  </div>
</header>
  <%-page.content%>
</article>

<div id="paginator">
  <%-paginator()%>
</div>
